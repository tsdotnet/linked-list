{
  "name": "@tsdotnet/linked-list",
  "version": "1.1.9",
  "description": "A doubly (bidirectional) linked list.  Acts as a safe, value focused wrapper for a linked-node-list.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/LinkedList.d.ts",
  "main": "dist/LinkedList.js",
  "module": "dist-esm/LinkedList.js",
  "keywords": [
    "linked list",
    "doubly linked list",
    "bi-directional",
    "bidirectional",
    "iterable"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && copyfiles -u 1 src/**/*.d.ts dist && tsc",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && run-p build:*",
    "bump": "npm run precommit && npm version patch",
    "docs": "rimraf docs/* && rimraf docs/.nojekyll && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run validate && git status -s",
    "prepublishOnly": "npm run build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run docs && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/linked-list.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/linked-list/issues"
  },
  "homepage": "https://github.com/tsdotnet/linked-list#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "chai": "^4.2.0",
    "copyfiles": "^2.3.0",
    "eslint": "^7.4.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.17.8",
    "typedoc-plugin-external-module-name": "3.1.0",
    "typedoc-plugin-nojekyll": "^1.0.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@tsdotnet/collection-base": "^2.0.6",
    "@tsdotnet/compare": "^1.3.13",
    "@tsdotnet/exceptions": "^1.0.16",
    "@tsdotnet/linked-node-list": "^1.3.13",
    "tslib": "^2.0.0"
  }
}
